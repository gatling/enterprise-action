    "package": "ncc build ./src/index.ts",
    "start": "ncc run ./src/index.ts",
    "test": "jest"
  },
  "dependencies": {
    "@actions/core": "1.10.0",
    "@actions/http-client": "2.0.1",
    "date-fns": "2.29.3",
    "idonttrustlikethat": "2.1.0"
  },
  "devDependencies": {
    "@types/node": "16.18.8",
    "@vercel/ncc": "0.36.0",
    "jest": "29.3.1",
    "js-yaml": "4.1.0",
    "nock": "13.2.9",
    "prettier": "2.8.1",
    "ts-jest": "29.0.3",
    "typescript": "4.9.4"
  }
}
