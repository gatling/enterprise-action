{
  "name": "gatling-enterprise-action",
  "description": "Gatling Enterprise GitHub Action",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gatling/enterprise-action.git"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/gatling/gatling/issues"
  },
  "homepage": "https://gatling.io/",
  "main": "dist/index.js",
