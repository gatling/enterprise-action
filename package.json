    "test": "jest"
  },
  "dependencies": {
    "@actions/core": "1.10.0"
  },
  "devDependencies": {
    "@types/node": "16.18.3",
    "@vercel/ncc": "0.34.0",
    "jest": "29.3.1",
    "js-yaml": "4.1.0",
    "prettier": "2.7.1",
    "ts-jest": "29.0.3",
    "typescript": "4.9.3"
  }
}
